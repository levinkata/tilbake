@model IEnumerable<QuoteItemViewModel>
@{
    
}

@if (Model.Any())
{
    <div class="form-group">
        <table class="table">
            <thead>
                <tr>
                    <th>
                        <strong>Description</strong>
                    </th>
                    <th>
                        <strong>Cover Type</strong>
                    </th>
                    <th>
                        <strong>Sum Insured</strong>
                    </th>
                    <th>
                        <strong>Premium</strong>
                    </th>
                    <th>
                        <strong>Excess</strong>
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Description</td>
                        <td>@item.CoverType</td>
                        <td>@item.SumInsured</td>
                        <td>@item.Premium</td>
                        <td>@item.Excess</td>
                        <td></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
