@model RatingMotorViewModel

@{
    ViewData["Title"] = Model.Insurer.Name;
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Rating Motor Details</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StartValue)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StartValue)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EndValue)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EndValue)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RateLocal)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RateLocal)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index" asp-route-insurerId="@Model.InsurerId">Return</a>
</div>
